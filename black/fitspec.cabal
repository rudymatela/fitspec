cabal-version:       >=1.10

library
  exposed-modules:   FitSpec, Mutate, Mutate.Show,
                     Test.Types.Mutate,
                     FitSpecC, Mutation
  other-modules:     Table, PPP,
                     Utils,
                     CatchEvaluation,
                     FitSpecDot
  build-depends:     base,  llcheck
  hs-source-dirs:    .
  default-language:  Haskell2010

benchmark bools
  main-is:           bools.hs
  build-depends:     base,  llcheck
  hs-source-dirs:    ., bench
  default-language:  Haskell2010
  type:              exitcode-stdio-1.0

benchmark spring
  main-is:           spring.hs
  build-depends:     base,  llcheck,  cmdargs
  hs-source-dirs:    ., bench
  default-language:  Haskell2010
